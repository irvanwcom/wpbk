<wpml-config>
  <custom-types>
    <custom-type translate="1">testimonials</custom-type>
    <custom-type translate="1">portfolio</custom-type>
  </custom-types>
  <taxonomies>
    <taxonomy translate="1">project-type</taxonomy>
  </taxonomies>
  <custom-fields>
    <custom-field action="copy">_cmb_theme_style</custom-field>
    <custom-field action="copy">_cmb_post_sidebar_layout</custom-field>
    <custom-field action="translate">name</custom-field>
    <custom-field action="translate">url</custom-field>
    <custom-field action="copy">portf_thumbnail</custom-field>
    <custom-field action="copy">link_type</custom-field>
    <custom-field action="translate">project_desc</custom-field>
    <custom-field action="translate">_cmb_projects-custom-fields</custom-field>
    <custom-field action="ignore">project_client</custom-field>
    <custom-field action="ignore">project_website</custom-field>
    <custom-field action="copy">_cmb_project_gallery_images</custom-field>
    <custom-field action="copy">_cmb_project_is_thumb_include</custom-field>
    <custom-field action="copy">_cmb_project_is_slider</custom-field>
    <custom-field action="translate">testimonial_content</custom-field>
    <custom-field action="translate">_cmb_alternate_title</custom-field>
    <custom-field action="copy">_cmb_menu_style</custom-field>


  </custom-fields>
  <admin-texts>
    <key name="theme_data">
      <key name="logo">
        <key name="url"/>
      </key>
      <key name="retina_logo">
        <key name="url"/>
      </key>
      <key name="favicon">
        <key name="url"/>
      </key>
      <key name="footer_text"/>
    </key>
  </admin-texts>
</wpml-config>